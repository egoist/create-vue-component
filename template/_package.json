{
  "name": "<%= name %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "repository": {
    "url": "<%= gitUser %>/<%= name %>",
    "type": "git"
  },
  "main": "dist/<%= name %>.common.js",
  "files": ["dist"],
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "test-vue-app",
    "build": "bili",
    "example": "poi",
    "build:example": "poi build"
  },
  "author": {
    "name": "<%= author %>",
    "email": "<%= email %>"
  },
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist",
    "homepage": "./"
  },
  "bili": {
    "format": ["cjs", "umd"],
    "plugins": ["vue"],
    "vue": {
      "css": "dist/<%= name %>.css"
    }
  },
  "devDependencies": {
    "poi": "^9.3.10",
    "bili": "^0.19.0",
    "test-vue-app": "^1.0.0",
    "vue-test-utils": "^1.0.0-beta.2",
    "rollup-plugin-vue": "^2.5.2"
  }
}
